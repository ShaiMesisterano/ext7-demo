{
  "name": "my-app",
  "product": "ext",
  "version": "0.0.1",
  "description": "my-ext-gen-app description for Ext JS app MyExtGenApp",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "keywords": [
    "Ext",
    "JS",
    "Sencha",
    "HTML5"
  ],
  "author": "Sencha, Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/"
  },
  "homepage": "http://www.sencha.com",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": ["eslint --fix", "git add"],
    "*.+(js|jsx|ts|tsx|json|css|md)": ["prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc", "git add"]
  },
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf build",
    "dev": "webpack-dev-server --env profile=desktop --env browser=yes --env verbose=no",
    "build": "npm run clean && cross-env webpack --env profile=desktop --env environment=production --env treeshake=yes",
    "build:testing": "npm run clean && cross-env webpack --env profile=desktop --env treeshake=yes --env cmdopts=--testing",
    "storybook": "start-storybook -p 6000",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "@sencha/ext": "~7.5.0",
    "@sencha/ext-modern": "~7.5.0",
    "@sencha/ext-modern-theme-material": "~7.5.0",
    "core-js": "^3.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@sencha/ext-webpack-plugin": "~7.5.0",
    "@storybook/addon-actions": "^6.5.0-alpha.46",
    "@storybook/addon-essentials": "^6.5.0-alpha.46",
    "@storybook/addon-interactions": "^6.5.0-alpha.46",
    "@storybook/addon-links": "^6.5.0-alpha.46",
    "@storybook/builder-webpack5": "^6.5.0-alpha.46",
    "@storybook/manager-webpack5": "^6.5.0-alpha.46",
    "@storybook/react": "^6.5.0-alpha.46",
    "@storybook/testing-library": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "portfinder": "^1.0.21",
    "prettier": "^2.6.0",
    "replace": "^1.1.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.7",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
}
